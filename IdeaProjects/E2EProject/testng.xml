<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
    <listeners>
        <listener class-name="Daraz.Common.listeners"/>
        <listener class-name="resources.ExtentReporterNG"/>
    </listeners>

    <test name="Landing Page Test">
        <classes>
            <class name="Daraz.LandingPageTest"/>
        </classes>
    </test>

    <test name="Login Page Test">
        <classes>
            <class name="Daraz.LoginPageTest"/>
        </classes>
    </test>

    <test name="Add To Cart Test">
        <classes>
            <class name="Daraz.AddToCartTest"/>
        </classes>
    </test>

    <test name="Delete From Cart Test">
        <classes>
            <class name="Daraz.DeleteCartItemsTest"/>
        </classes>
    </test>


</suite>
